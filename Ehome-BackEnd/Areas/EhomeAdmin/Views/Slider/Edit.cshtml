@using Ehome_BackEnd.Models
@model Slider
@{
    ViewData["Title"] = "Edit";
}

<form method="post">
    <div>
        <label class="mb-3">Slider Image:</label><br />
        <input asp-for="Photo" class="form-control" accept="Image/*"/><br />
        <span class="text-danger" asp-validation-for="Photo"></span>
    </div>
    <div class="mt-3">
        <button type="submit" class="btn btn-primary">Edit slider</button>
    </div>
</form>

